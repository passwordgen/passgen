{
  "manifest_version": 2,
  "name": "Password Generator",
  "version": "1.1",
  "description": "Generates a random password and copies it to the clipboard",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite"
  ],
  "web_accessible_resources": [
    "popup.css"
  ]
}