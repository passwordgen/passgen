{
  "manifest_version": 2,
  "name": "Password generator",
  "version": "1.1.1",
  "author": "xtyl",
  "homepage_url": "https://github.com/x1yl",
  "description": "Generates a random password and copies it to the clipboard",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "128": "icons/icon128.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "24": "icons/icon24.png"
  },
  "permissions": [
    "clipboardWrite",
    "tabs"
  ],
  "web_accessible_resources": [
    "popup.css"
  ]
}